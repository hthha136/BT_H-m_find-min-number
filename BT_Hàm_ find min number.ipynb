    "def min_number(numbers):\n",
    "    result = numbers[0]\n",
    "    for i in numbers:\n",
    "        if result > i:\n",
    "            result = i\n",
    "    return result\n",
    "numbers = [8,5,4,-8,6,-10]\n",
    "print('min: ',min_number(numbers))"
